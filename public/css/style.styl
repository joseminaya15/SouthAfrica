$regular   = 'MetricRegular'
$bold      = 'MetricBold'
$medium    = 'MetricSemiBold'
$light     = 'MetricLight'
$color   = #FDB913
$default = #01a982
flexCenter()
	display flex
	display -webkit-flex
	align-items center
flexStart()
	display flex
	display -webkit-flex
	align-items start
flexEnd()
	display flex
	display -webkit-flex
	align-items flex-end
transitionAll()
	-webkit-transition all .3s linear
	-moz-transition all .3s linear
	-ms-transition all .3s linear
	-o-transition all .3s linear
	transition all .3s linear
transitionOpacity()
	-webkit-transition opacity .3s linear
	-moz-transition opacity .3s linear
	-ms-transition opacity .3s linear
	-o-transition opacity .3s linear
	transition opacity .3s linear
transitionWidth()
	-webkit-transition width .3s linear
	-moz-transition width .3s linear
	-ms-transition width .3s linear
	-o-transition width .3s linear
	transition width .3s linear
transitionMaxWidth()
	-webkit-transition max-width .3s linear
	-moz-transition max-width .3s linear
	-ms-transition max-width .3s linear
	-o-transition max-width .3s linear
	transition max-width .3s linear
transitionBgColor()
	-webkit-transition background-color .3s linear
	-moz-transition background-color .3s linear
	-ms-transition background-color .3s linear
	-o-transition background-color .3s linear
	transition background-color .3s linear
transitionTransform()
	-webkit-transition transform .3s linear
	-moz-transition transform .3s linear
	-ms-transition transform .3s linear
	-o-transition transform .3s linear
	transition transform .3s linear
absolute()
	position absolute
	top 0
	left 0
	right 0
	bottom 0
relative()
	position relative
	top 0
	left 0
	right 0
	bottom 0
flipCard()
	-moz-backface-visibility hidden
	-webkit-backface-visibility hidden
	backface-visibility hidden
	-webkit-transition -webkit-transform 500ms
	-moz-transition -moz-transform 500ms
	-o-transition -o-transform 500ms
	transition transform 500ms
	border-radius 5px
	position absolute
flipCardFront()
	-webkit-transform perspective(600) rotateY(0)
	-moz-transform perspective(600) rotateY(0)
	-o-transform perspective(600) rotateY(0)
	transform perspective(600) rotateY(0)
flipCardBack()
	-webkit-transform rotateY(-180deg)
	-moz-transform rotateY(-180deg)
	-o-transform rotateY(-180deg)
	transform rotateY(-180deg)
	z-index 900
flipCardFrontHover()
	-webkit-transform rotateY(180deg)
	-moz-transform rotateY(180deg)
	-o-transform rotateY(180deg)
	transform rotateY(180deg)
	-webkit-transform perspective(600) rotateY(180deg)
	-moz-transform perspective(600) rotateY(180deg)
	-o-transform perspective(600) rotateY(180deg)
	transform perspective(600) rotateY(180deg)
flipCardBackHover()
	-webkit-transform rotateY(0)
	-moz-transform rotateY(0)
	-o-transform rotateY(0)
	transform rotateY(0)
	-webkit-transform perspective(600) rotateY(0)
	-moz-transform perspective(600) rotateY(0)
	-o-transform perspective(600) rotateY(0)
	transform perspective(600) rotateY(0)
	z-index 950
body
	margin 0
	padding 0
	overflow hidden
	overflow-y scroll
	font-family $regular
	color #000000
	background-color #EFEFEF
img
	width 100%
p
	margin 0
	font-size 18px
strong
	font-family $medium
.js-section
	position relative
	padding 40px 20px
.js-white
	background-color #FFFFFF
.js-title
	text-align center
	font-size 32px
	font-family $medium
	margin 20px 0
.js-flex
	flexCenter()
	justify-content center
.js-flex--start
	flexStart()
	justify-content center
.js-height
	height 100vh
.js-header
	transitionAll()
	position fixed
	top 0
	width 100%
	background-color #425563
	z-index 5
	padding 5px 25px
	color #FFFFFF
	height 75px
	&.js-fixed
		background-color $color
	.js-header--container
		flexCenter()
		width 100%
		max-width 1150px
		margin auto
	img
		width 100%
		max-width 115px
		position absolute
		top 5px
	.js-header--left
		flexCenter()
		width 100%
		max-width 300px
		justify-content flex-start
	.js-header--right
		flexCenter()
		width 100%
		justify-content flex-end
		text-align right
.js-container
	transitionMaxWidth()
	height 100%
	width 100%
	max-width 1150px
	margin auto
.js-input
	padding 7px 15px
	height 70px
	margin 0
	border 1px solid rgba(0,0,0,0.15)
	background-color #FFFFFF
	margin-bottom -1px
	color #333333
	margin 5px 0
	&.js-date
		padding 5px 15px
		.js-icon
			width 100%
			max-width 36px
			text-align center
	&.js-file
		height 100%
	&.js-radio
		height 100%
		.mdl-radio
			width 100%
			.mdl-radio__inner-circle
				background $default
			&.is-checked
				.mdl-radio__outer-circle
					border 2px solid $default
			.mdl-radio__ripple-container 
				.mdl-ripple
					background $default
	&.js-select
		padding 0
		.bootstrap-select
			width 100% !important
			height 100%
			.dropdown-toggle
				padding 14px 15px
				border-radius 0
				box-shadow none
				background-color transparent
				border none
				height 100%
			.dropdown-menu
				background-color rgba(248, 248, 248, 0.95)
				border-radius 0
				margin 0 
				padding 0
				&.open
					margin 0
					padding 0
					li
						a
							padding 6px 20px
							&:hover
								background-color rgba(221,221,221,0.5)
	label
		font-family $light
		color #666666
		cursor pointer
		margin-bottom 0
		font-weight 100
	input
		display block
		height 36px
		width 100%
		border none
		font-size 16px
		&::placeholder
			font-size 15px
		&:focus
			outline 0
		&[disabled]
			background-color transparent
.js-select
	&.js-standar
		margin 5px 0
		.bootstrap-select
			width 100% !important
			.btn-default
				background-color #FFFFFF
				color #757575
				height 46px
				border-radius 0	
.js-contact-label
	flexCenter()
	flex-wrap wrap
	.js-input-label
		width 100%
		max-width 120px
.js-label
	height 145px
	margin 5px 0
	span
		font-family $bold
		a
			color #008FD3 !important
	.mdl-checkbox__label
		font-size 14px
		color #000000
		line-height 18px
		font-family $regular
		font-weight 100
.js-button
	background-color $default !important
	text-decoration none !important
	font-size 16px
	color #FFFFFF !important
	border 2px solid $default
	margin 10px
	font-family $medium
	width 100%
	max-width 160px
	&.js-button--default
		background-color transparent !important
		border 2px solid #FFFFFF
.mdl-radio__inner-circle
	background #4983B6
.mdl-checkbox
	&.is-checked 
		.mdl-checkbox__tick-outline
			background #4983B6
.mdl-checkbox__ripple-container 
	&.mdl-ripple
		background #4983B6
.mdl-radio__ripple-container 
	.mdl-ripple
		background #4983B6
.mdl-checkbox
	&.is-checked 
		.mdl-checkbox__box-outline
			border 2px solid #4983B6
.mdl-radio
	&.is-checked 
		.mdl-radio__outer-circle
			border 2px solid #4983B6
.js-checkbox
	.mdl-checkbox
		max-width 250px
	.mdl-checkbox__label
		font-size 15px
		line-height 18px
		font-weight 100
	.mdl-checkbox__box-outline
		border 2px solid #FFFFFF
.video
video
	position absolute
	top 50%
	left 0
	-webkit-transform translate( 0 , -50% )
	transform translate( 0 , -50% )
	min-height 100%
	min-width 100%
	z-index 0
	overflow hidden
	display block
.video
	display none
	&:AFTER
		position absolute
		content ''
		top 0
		right 0
		left 0
		z-index 2
		background linear-gradient(#FFFFFF, transparent)
		width 100%
		height 200px
.js-fondo--home
	display block
.js-fondo
	absolute()
	background url(../img/fondo/fondo-home.jpg) no-repeat center center
	background-size cover !important
	width 100%
	height 100vh
	z-index -1
	&:BEFORE
		absolute()
		content ''
		background-color #000000
		z-index 0
		height 100vh
		opacity .2
	&:AFTER
		position absolute
		content ''
		top 0
		right 0
		left 0
		z-index 2
		background linear-gradient(#FFFFFF, transparent)
		width 100%
		height 200px
	&.fondo1
		background url(../img/fondo/fondo1.jpg) no-repeat center center
	&.fondo2
		background url(../img/fondo/fondo2.jpg) no-repeat center center
	&.fondo3
		background url(../img/fondo/fondo3.jpg) no-repeat center center
#principal
	.owl-stage-outer
	.owl-stage
		height 100vh
	.owl-carousel
		absolute()
		width 100%
		z-index 1
		max-width 100%
		.owl-item
			height 100vh
			&.active
				.item
					.fondo1
						-webkit-animation js-animation1 10s infinite
						animation js-animation1 10s infinite
					.fondo2
						-webkit-animation js-animation2 10s infinite
						animation js-animation2 10s infinite
					.fondo3
						-webkit-animation js-animation1 10s infinite
						animation js-animation1 10s infinite
			.item
				height 100vh
footer
	background-color #425563
	color #FFFFFF
	p
		font-size 14px
.nav-tabs
	border none
	&>li
		&>a
			border none !important
			font-size 18px
	&>li
		&.active
			&>a
				font-family $bold				
/*CLASES PROYECTO*/
.js-contenido
	text-align center
	color #FFFFFF
	max-width 600px
	z-index 1
	h2
		font-size 55px
		line-height 55px
		font-family $bold
	p
		font-size 32px
		line-height 32px
.js-information
	h2
		font-family $medium
	p
		line-height 20px
		margin 15px 0
	strong
		margin 10px 0
		font-size 18px		
	ul
		li
			font-size 18px
.js-nav
	a
		img
			position absolute
			max-width 125px
.js-card
	box-shadow 0 2px 5px rgba(0,0,0,0.16), 0 2px 5px rgba(0,0,0,0.23)
	border-radius 5px
	padding 15px
	margin 20px 0
	background-color #FFFFFF
.js-confirmation
	width 100%
	max-width 600px
	color #000000
	text-align center
	margin 40px auto
	display none
	&.aparecer
		display block
	h2
		font-family $medium
		font-size 40px
		line-height 40px
		margin 20px 0
	p
		letter-spacing 1px
	strong
		display block
		font-size 18px
/*MATERIAL DESIGN*/
.mdl-card
.mdl-card__supporting-text
	width 100%
/*MODAL*/
.modal
	&.fade
		.modal-dialog
			transform translate(-50% , -50%)
			position absolute
			top 50%
			left 50%
			.mdl-card__actions
				padding 16px
				button
					text-transform initial
					font-family $medium
		&.in
			.modal-dialog
				transform translate(1)
				transform transform(-50% , -50%)
				opacity 1
/*ANIMACIONES*/
@-webkit-keyframes js-animation1
	0%
		-webkit-transform translate3d(0px, 0px, 0px) scale(1)
		-moz-transform translate3d(0px, 0px, 0px) scale(1)
		-ms-transform translate3d(0px, 0px, 0px) scale(1)
		-o-transform translate3d(0px, 0px, 0px) scale(1)
		transform translate3d(0px, 0px, 0px) scale(1)
	100%
		-webkit-transform translate3d(-100px, 0px, 0px) scale(1.2)
		-moz-transform translate3d(-100px, 0px, 0px) scale(1.2)
		-ms-transform translate3d(-100px, 0px, 0px) scale(1.2)
		-o-transform translate3d(-100px, 0px, 0px) scale(1.2)
		transform translate3d(-100px, 0px, 0px) scale(1.2)
@keyframes js-animation1
	0%
		-webkit-transform translate3d(0px, 0px, 0px) scale(1)
		-moz-transform translate3d(0px, 0px, 0px) scale(1)
		-ms-transform translate3d(0px, 0px, 0px) scale(1)
		-o-transform translate3d(0px, 0px, 0px) scale(1)
		transform translate3d(0px, 0px, 0px) scale(1)
	100%
		-webkit-transform translate3d(-100px, 0px, 0px) scale(1.2)
		-moz-transform translate3d(-100px, 0px, 0px) scale(1.2)
		-ms-transform translate3d(-100px, 0px, 0px) scale(1.2)
		-o-transform translate3d(-100px, 0px, 0px) scale(1.2)
		transform translate3d(-100px, 0px, 0px) scale(1.2)
@-webkit-keyframes js-animation2
	0%
		-webkit-transform translate3d(0px, 0px, 0px) scale(1)
		-moz-transform translate3d(0px, 0px, 0px) scale(1)
		-ms-transform translate3d(0px, 0px, 0px) scale(1)
		-o-transform translate3d(0px, 0px, 0px) scale(1)
		transform translate3d(0px, 0px, 0px) scale(1)
	100%
		-webkit-transform translate3d(100px, 0px, 0px) scale(1.2)
		-moz-transform translate3d(100px, 0px, 0px) scale(1.2)
		-ms-transform translate3d(100px, 0px, 0px) scale(1.2)
		-o-transform translate3d(100px, 0px, 0px) scale(1.2)
		transform translate3d(100px, 0px, 0px) scale(1.2)
@keyframes js-animation2
	0%
		-webkit-transform translate3d(0px, 0px, 0px) scale(1)
		-moz-transform translate3d(0px, 0px, 0px) scale(1)
		-ms-transform translate3d(0px, 0px, 0px) scale(1)
		-o-transform translate3d(0px, 0px, 0px) scale(1)
		transform translate3d(0px, 0px, 0px) scale(1)
	100%
		-webkit-transform translate3d(100px, 0px, 0px) scale(1.2)
		-moz-transform translate3d(100px, 0px, 0px) scale(1.2)
		-ms-transform translate3d(100px, 0px, 0px) scale(1.2)
		-o-transform translate3d(100px, 0px, 0px) scale(1.2)
		transform translate3d(100px, 0px, 0px) scale(1.2)
@keyframes js-animation0
	0%
		-webkit-transform scale(1)
		-moz-transform scale(1)
		-ms-transform scale(1)
		-o-transform scale(1)
		transform scale(1)
	100%
		-webkit-transform scale(1.2)
		-moz-transform scale(1.2)scale(1.2)
		-ms-transform scale(1.2)
		-o-transform scale(1.2)
		transform scale(1.2)
@-webkit-keyframes js-animation0
	0%
		-webkit-transform scale(1)
		-moz-transform scale(1)
		-ms-transform scale(1)
		-o-transform scale(1)
		transform scale(1)
	100%
		-webkit-transform scale(1.2)
		-moz-transform scale(1.2)
		-ms-transform scale(1.2)
		-o-transform scale(1.2)
		transform scale(1.2)
table
	&>tbody
		&>tr
			&>td
				font-size 16px
				vertical-align middle !important
.table-responsive
	border none
/*SCROLL*/
::-webkit-scrollbar
	width 5px
	height 5px
::-webkit-scrollbar-track
	background-color transparent
	border-radius 10px
::-webkit-scrollbar-thumb
	background-color #AAAAAA
	border-radius 10px
@media (min-width: 768px)
	.js-header
		background-color transparent
		img
			&.logo-one
				opacity 1
			&.logo-two
				opacity 0
		&.navbarcolor
			background-color #425563
			img
				&.logo-one
					opacity 0
				&.logo-two
					opacity 1
	.navbar-brand
		img
			max-width 250px
	.js-fondo--home
		display none
	.video
		display block
@media (max-width: 1000px)
	.navbar-brand
		small
			display none
	#principal
		.owl-carousel
			.owl-item
				&.active
					.item
						.fondo1
						.fondo2
						.fondo3
							-webkit-animation js-animation0 10s infinite
							animation js-animation0 10s infinite
@media (max-width: 600px)
	.js-contenido
		padding 40px 20px
		h2
			font-size 46px
			line-height 48px